        <div class="logo">
            <a href="/">
                <img src="/static/img/GYxzqd1.png" alt="">
            </a>
        </div>
        <div class="row">
            <div class="col-md-4 qcol-info">
               <div class="qcol-info_line info_line_hover1"></div>
